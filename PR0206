#include <stdio.h>
#include <stdlib.h>

int main()
{
    float a,b,c,d,e;

    scanf("%f %f %f %f %f", &a, &b, &c, &d, &e);
    
    if ( (a < b) && (a < c) && (a<d) && (a<e) )
    {
        printf("1");
    }
    else if ( (b < a) && (b < c) && ( b < d ) && (b < e) )
    {
        printf("2");
    }
    else if ( (c < a) && (c < b) && ( c < d ) && (c < e))
    {
        printf("3");
    }
    else if ( (d < a) && (d < c) && ( d < b ) && (d < e) )
    {
        printf("4");
    }
    else if ( (e < a) && (e < b) && ( e < c ) && (e < d) )
    {
        printf("5");
    }

    return 0;
}
