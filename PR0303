#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,i,j,count = 0, ni,pi,nip;
    scanf("%d", &n);
    for(i=2; i + i<= n; i++)
    {
        ni = n - i;
        pi = 1;
        for(j=2;pi && j*j<=i;j++)
        {
            if (i%j ==0)
                pi=0;
        }
        nip=1;
        for (j=2;nip && j*j<= ni; j++)
        {
            if (ni % j ==0)
                nip = 0;
        }
        if (pi && nip)
            count++;
    }
    printf("%d", count);

    return 0;
}
