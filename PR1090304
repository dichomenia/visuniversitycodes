#include <iostream>

using namespace std;

int main()
{
    //cout << "Hello world!" << endl;

    int num1,num2;
    //cout << "Enter 2 numbers:" << endl;
    cin >> num1 >> num2;

    //cout << "The entered numbers were:" << endl;
    //cout << num1 << " " << "and " << num2 << " " <<endl;

    cout << num1 << " " << "+ " << num2 << " " << "= " << num1+num2 << endl;
    cout << num1 << " " << "- " << num2 << " " << "= " << num1-num2 << endl;
    cout << num1 << " " << "* " << num2 << " " << "= " << num1*num2 << endl;
    cout << num1 << " " << "/ " << num2 << " " << "= " << num1/num2 << endl;
    cout << num1 << " " << "% " << num2 << " " << "= " << num1%num2 << endl;


    return 0;
}
