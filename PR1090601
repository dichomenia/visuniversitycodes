#include <stdio.h>
#include <stdlib.h>
#define row 19
#define column 19

int main()
{
   char gomoku[row][column];
   char winner;

   for(int i=0;i<row;i++)  //input
   {
       for(int j=0;j<column;j++)
       {
           scanf("%c",&gomoku[i][j]);
       }
       getchar();
   }

   for(int i=0;i<row;i++)
   {
       for(int j=0;j<column;j++)
       {


           if(gomoku[i][j]!='_')
           {
               if(i+4<=18 && j+4<=18) //pass this is off the board
               {
                     if(gomoku[i][j]==gomoku[i+1][j+1] && gomoku[i+1][j+1]==gomoku[i+2][j+2] && gomoku[i+2][j+2]==gomoku[i+3][j+3] && gomoku[i+3][j+3]==gomoku[i+4][j+4])
                     {
                         winner=gomoku[i][j];
                         break;
                     }
               }
               if(i+4<=18 && j-4>=0)
               {
                    if(gomoku[i][j]==gomoku[i+1][j-1] && gomoku[i+1][j-1]==gomoku[i+2][j-2] && gomoku[i+2][j-2]==gomoku[i+3][j-3] && gomoku[i+3][j-3]==gomoku[i+4][j-4])
                    {
                       winner=gomoku[i][j];
                       break;
                    }
               }
               if(gomoku[i][j]==gomoku[i][j+1] && gomoku[i][j+1]==gomoku[i][j+2] && gomoku[i][j+2]==gomoku[i][j+3] && gomoku[i][j+3]==gomoku[i][j+4]) //right
               {
                   winner=gomoku[i][j];
                   break;
               }
               if(gomoku[i][j]==gomoku[i+1][j] && gomoku[i+1][j]==gomoku[i+2][j] && gomoku[i+2][j]==gomoku[i+3][j] && gomoku[i+3][j]==gomoku[i+4][j] )//down
               {
                   winner=gomoku[i][j];
                   break;
               }
           }


       }
   }

    if(winner=='O')
        printf("White");
    else if (winner=='X')
        printf("Black");
    else
        printf("No winner");
return 0;
}

