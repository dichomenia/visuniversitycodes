#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int main()
{
    int matrix[100][100], row, columns, amount;
    int i, j;
    int zero = 0;
    int nrow, ncolumns, num;

    //taking the rows column and numbers you want to change
    cin >> row >> columns >> amount;

    //creating sparse matrix
    for (i = 0; i < row; i++)
    {
        for (j = 0; j < columns; j++)
        {
            matrix[i][j] = zero;
        }
    }

    //loop reading where to enter the new number
    for (i = 0; i < amount; i++)
    {
        //reading row
        cin >> nrow;
        //reading column
        cin >> ncolumns;
        //reading new number
        cin >> num;
        //assigning the new number in the position
        matrix[nrow][ncolumns] = num;
    }

    //printing matrix
    for (i = 0; i < row; i++)
    {
        for (j = 0; j < columns; j++)
        {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}

/*
Test case
5 6 4
1 1 3
2 3 6
3 2 9
4 4 12

Output
0 0 0 0 0 0
0 3 0 0 0 0
0 0 0 6 0 0
0 0 9 0 0 0
0 0 0 0 12 0
*/
