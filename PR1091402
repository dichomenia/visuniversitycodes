#include <stdio.h>



int main()
{


int num,num2;
char ch1,ch2,ch3;
//cin >> num;
scanf("%d", &num);

//while(cin >> ch1>>ch2>>ch3)
while(scanf("%c%c%c", &ch1, &ch2, &ch3)==3)
{
    //cin >> num2;
    scanf("%d", &num2);
    if(ch2 =='+')
    {
        num += num2;
    }else if(ch2 =='-')
    {
        num -= num2;
    }else if (ch2 =='*')
    {
        num *= num2;
    }else if(ch2 =='/')
    {
        num /= num2;
    }


}

//cout << num;
printf("%d", num);

    return 0;
}

//1 + 2 * 3 / 4
