#include <iostream>

using namespace std;

int main()
{
    int matrix[100][100];
    int i,j, zero = 0;
    int row, columns, amount;
    int nrow, ncolumns, num;


    //cout << "Enter the row in the matrix:\n";
    cin >> row;

    //cout << "Enter the columns in the matrix:\n";
    cin >> columns;

    //cout << "How many numbers do you want to change?\n";
    cin >> amount;

    //creating sparse matrix
    for (i = 0; i < row; i++)
    {
        for (j = 0; j < columns; j++)
        {
            matrix[i][j] = zero;
        }
    }

    for (i=1; i<=amount; i++)
    {
        //cout << "\nNumber " << i << endl;
        //cout << "Enter the row for the new number:\n";
        //reading row
        cin >> nrow;

        //cout << "\nEnter the column for the new number:\n";
        //reading column
        cin >> ncolumns;

        //cout << "\nEnter the new number:\n";
        //reading new number
        cin >> num;

        //assigning the new number in the position
        matrix[nrow][ncolumns] = num;
    }

    //printing matrix
    //cout << "\nThe matrix is:\n";
    for (i = 0; i < row; i++)
    {
        for (j = 0; j < columns; j++)
        {
            cout << matrix[i][j] << " ";
        }
        cout << "\n";
    }
    return 0;
}
